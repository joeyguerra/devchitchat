    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "debug": "^4.3.4",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "jws": "^4.0.0",
    "method-override": "^3.0.0",
    "mime": "^3.0.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
